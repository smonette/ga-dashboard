<div class="twittericon"></div>
<div class="gabox">
<div class="tweetbox">
  <% @GA_SF.each do |tweet| %>
  <div class="tweet"><p><div class="galogo"></div><a class="twitterlink" href="https://twitter.com/GA_SF"><strong class="twittertitle"> General Assembly SF</strong> </a><span class="handle"> @GA_SF</span><br> <span class="time"><%= tweet.created_at.in_time_zone('Pacific Time (US & Canada)').strftime(" %B %d, %Y %I:%M %p") %></span></br><%= twitter_text(tweet.full_text) %></p></div>
  <% end %>
</div>
</div>